{{!-- {{jsonPrint this}} --}}
<div class="cirb banner border-clear">
	<div class="container-fluid">
		{{!-- <div class="row"></div> --}}
		<div class="row irb-proj">
			<div class="col-md-8 col-sm-8">
			{{!-- {{jsonPrint irb_proj}} --}}
				{{!-- {{> page/name name="Vision"}} --}}
				<div class="title-white irb-description">
					<span id="markdown-span" class="irb-proj-title">{{{irb_proj.projectTitle.html}}}</span>
					<span id="markdown-span">{{{irb_proj.projDescription.html}}}</span>
				</div>
			</div>
			<div class="col-md-4 col-sm-4">

				<div class="text-gray-dark irb-image">
					<h3>{{irb_proj.imageTitle}}</h3>
					<img src="{{{cloudinaryUrl irb_proj.image width=250 height=250 crop='scale' quality=100}}}">
					<p>{{irb_proj.imageCaption}}</p>
					<p>Contact: {{emailFormat irb_proj.email}}</a></p>
				</div>
			</div>
			{{!-- <div class="row"></div> --}}
		</div>
	</div>
</div>
