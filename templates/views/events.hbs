<section id="events">
	<div class="container-fluid" style="padding: 0px">
	<div class="col-sm-12 col-md-6 col-centered">

	{{#each events}}
		{{#if enabled}}
			{{#if featured}}
					{{>events/featured event=this}}
			{{/if}}
		{{/if}}
	{{/each}}
	
	</div>

	<div class="talks-grid">
		<div class="row">
			<h1 class="page-title text-center" style="padding-bottom:3%;">Past Events</h1>

				{{#each events}}
					{{#if enabled}}
							{{>index/recent_event event=this}}
					{{/if}}
				{{/each}}

		</div>
			
	</div>
		
	</div>
</section>
