<section id="events">

	{{#if events}}
	<div class="events">
		<div class="col-xs-12 col-sm-10 col-md-10 col-lg-8 col-centered">
			<h1 class="page-title text-center" style="padding-bottom:3%;">Events</h1>
			{{#each events}}
				{{#if enabled}}
					{{>index/recent_event event=this}}
				{{/if}}
			{{/each}}			
		</div>
	</div>
	{{/if}}

</section>
