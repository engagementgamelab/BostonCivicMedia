<section id="events">
	<div class="container-fluid" style="padding: 0px">
		<div class="row events-feature">
			<div class="col-md-12">
				{{!-- <h1 class="page-title text-center">Featured Event:</h1> --}}

			{{#each events}}
				{{#if enabled}}
					{{#if featured}}
							{{>index/featured_event event=this}}
					{{/if}}
				{{/if}}
			{{/each}}
			
			</div>
		</div>

		<div class="talks-grid">
			<div class="row">
				<h1 class="page-title text-center" style="padding-bottom:3%;">Past Events</h1>

					{{#each events}}
						{{#if enabled}}
								{{>index/recent_event event=this}}
						{{/if}}
					{{/each}}

			</div>
				
		</div>
		
	</div>
</section>
