<section id="events">

	{{#each events}}
		{{#if enabled}}
			{{#if featured}}
					{{>events/featured event=this}}
			{{/if}}
		{{/if}}
	{{/each}}

	<div class="col-xs-12 col-sm-10 col-md-10 col-lg-10 col-centered">
		<h1 class="page-title text-center" style="padding-bottom:3%;">Past Events</h1>

			{{#each events}}
				{{#if enabled}}
						{{>index/recent_event event=this}}
				{{/if}}
			{{/each}}			
	</div>

</section>
