<footer id="footer" class="row clearfix">

	<div class="footer col-lg-6 col-sm-12 col-centered">
		<div class="dark footer-content text-center padding-ver-clear">

			<div class="logos-footer center-block">
				{{{cloudinaryUrl '/co_rgb:85ddd2,e_colorize:100,f_png/v1495219010/boston-civic-media/layout/el_logo_white' height=75 format='svg' id="logo-el" alt="Engagement Lab"}}}
				{{{cloudinaryUrl '/co_rgb:85ddd2,e_colorize:100,f_png/v1495219010/boston-civic-media/layout/cross' width=35 format='svg' alt="Boston Civic Media"}}}
				{{{cloudinaryUrl '/co_rgb:85ddd2,e_colorize:100,f_png/v1495219010/boston-civic-media/layout/bcm_logo_white' height=75 format='svg' id="logo-bcm" alt="Boston Civic Media"}}}
				{{{cloudinaryUrl '/co_rgb:85ddd2,e_colorize:100,f_png/v1496158923/boston-civic-media/layout/cross' width=35 format='svg' alt="Boston Civic Media"}}}
				{{{cloudinaryUrl '/co_rgb:85ddd2,e_colorize:100,f_png/v1496181556/boston-civic-media/layout/teagle_white' height=75 format='svg' id="logo-teagle" alt="Teagle Foundation" class=""}}}
			</div>

			<div class="row footer-content-list">
				<div class="col-md-12">
					{{#each footer.content}}
					<div class="footer-line">{{this}}</div>
					{{/each}}
				</div>
			</div>

			<div class="row buttons">
				<div class="subscribe col-md-5 col-centered">
					
					<a href="{{footer.subscribeLink}}" class="btn subscribe-button">Newsletter</a>
					<p>{{footer.subscribe}}</p>
				</div>

				<div class="subscribe col-md-5 col-centered">
					
					<a href="{{footer.listservLink}}" class="btn subscribe-button">Listserv</a>
					<p>{{footer.listserv}}</p>
				</div>
			</div>
							
		</div>
	</div>

</footer>